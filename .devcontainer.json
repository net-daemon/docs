{
  "name": "NetDaemon Documentation",
  "image": "ghcr.io/ludeeus/devcontainer/frontend:latest",
	"settings": { 
		"terminal.integrated.shell.linux": "/bin/bash",
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.formatOnSave": false,
		"[javascript]": {
		  "editor.defaultFormatter": "esbenp.prettier-vscode",
		  "editor.formatOnSave": false
		  
	  }
